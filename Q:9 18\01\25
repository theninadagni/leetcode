PALENDROM
  

class Solution {
public:
    bool isPalindrome(int x) {
        if (x<0)
        {return false;}
        int reversed =0, temp=0;
        while (temp!=0)
        {int digit =temp%10;
        reversed =reversed *10+digit ;
        temp=temp/10;
        }
        return (reversed == x);
    }

};
